<UserControl x:Class="WpfApplication2.SetupEventView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:views="clr-namespace:WpfApplication2"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="300">
	<UserControl.Resources>
		<views:SetupEventDataTemplateSelector x:Key="keySetupEventSelector" />
	</UserControl.Resources>
    <Grid>
		<ContentControl ContentTemplateSelector="{StaticResource keySetupEventSelector}" />
    </Grid>
</UserControl>
